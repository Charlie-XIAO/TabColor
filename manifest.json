{
    "manifest_version": 2,
    "name": "Color Tabs",
    "version": "1.0",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "activeTab"
    ], 
    "content_scripts": [
    {
        "js": ["background.js"],
        "matches": ["<all_urls>"]
    }
    ]
} 

